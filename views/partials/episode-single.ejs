<div class="episode-single <%= className %>">
    <div class="wrapper">
        <div class="episode-cover">
            <img src="<%= episode.image %>" alt="cover">
        </div>
        <div class="episode-info">
            <h2><%= episode.title %></h2>
        </div>
        <div class="episode-description">
            <div class="rich-text">
                <%- episode.md %>
            </div>
        </div>
    </div>
</div>
